# This workflow deploys serverless for <HELPLINE> to its <ENVIRONMENT> environment

name: <HELPLINE> <ENVIRONMENT> deploy

on:
  workflow_dispatch
jobs:
  deploy:
    uses: ./.github/workflows/custom_helpline.yml
    secrets: inherit
    with:
      helpline_code: "<SHORT_HELPLINE>"
      environment_code: "<SHORT_ENVIRONMENT>"
      changelog: "Deploying individual account"
