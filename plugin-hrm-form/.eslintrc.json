{
  "extends": [
    "twilio-react"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module", 
    "ecmaFeatures": { 
    "jsx": true
   }
  },
  "rules": {
    "no-console": "off",
    "no-warning-comments": "off",
    "no-confusing-arrow": "off",
    "no-alert": "off",
    "import/no-extraneous-dependencies": "off",
    "jsx-a11y/tabindex-no-positive": "off",
    "sonarjs/no-duplicate-string": "off"
  }
}