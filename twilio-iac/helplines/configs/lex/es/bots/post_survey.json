{
  "post_survey": {
    "description": "General configs/lex/es/bots Post Survey bot",
    "locale": "es-419",
    "process_behavior": "BUILD",
    "child_directed": true,
    "idle_session_ttl_in_seconds": "600",
    "enable_model_improvements": true,
    "abort_statement": {
      "content": "Lo siento, no puedo ayudarte en este momento. Te transferiré a un consejero",
      "content_type": "PlainText"
    },
    "clarification_prompt": {
      "max_attempts": 2,
      "content": "Lo siento, no entendí eso. Por favor, inténtalo nuevamente.",
      "content_type": "PlainText"
    },
    "intents": [
      "post_survey_yes",
      "post_survey_no"
    ]
  }
}