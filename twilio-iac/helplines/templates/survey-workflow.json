{
  "task_routing": {
    "filters": [
      {
        "filter_friendly_name": "${helpline}",
        "expression": "1==1 : helpline IN [\"${helpline}\"] OR channelType ==\"web\" OR isContactlessTask == true",
        "targets": [
          {
            "expression": "isSurveyTask==true",
            "queue": "${task_queues.survey}"
          }
        ]
      }
    ]
  }
}