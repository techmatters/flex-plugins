        "message_type": "custom",
        "to": "{{contact.channel.address}}",
        "body": "${flow_vars.courage_first_sms_failure_message}"
      }
    },
    {
      "name": "send_message_courage_first_sms_prequeue",
      "type": "send-message",
      "transitions": [
        {
          "next": "send_to_queue_courage_first",
          "event": "sent"
        },
        {
          "event": "failed",
          "next": "send_to_queue_courage_first"
        }
      ],
      "properties": {
        "offset": {
          "x": 1240,
          "y": 1640
        },
        "service": "{{trigger.message.InstanceSid}}",
        "channel": "{{trigger.message.ChannelSid}}",
        "from": "Bot",
        "message_type": "custom",
        "to": "{{contact.channel.address}}",
        "body": "${flow_vars.courage_first_sms_prequeue_message}"
      }
    },
    {
      "name": "send_message_courage_first_sms_redirect",
      "type": "send-message",
      "transitions": [
        {
          "event": "sent"
        },
        {
          "event": "failed"
        }
      ],
      "properties": {
        "offset": {
          "x": 20,
          "y": 1690
        },
        "service": "{{trigger.message.InstanceSid}}",
        "channel": "{{trigger.message.ChannelSid}}",
        "from": "Bot",
        "message_type": "custom",
        "to": "{{contact.channel.address}}",
        "body": "${flow_vars.courage_first_sms_redirect_message}"
      }
    },
    {
      "name": "run_subflow_system_down",
      "type": "run-subflow",
      "transitions": [
        {
          "next": "split_pre_survey_complete",
          "event": "completed"
        },
        {
          "next": "split_pre_survey_complete",
          "event": "failed"
        }
      ],
      "properties": {
        "flow_sid": "${system_down_studio_subflow_sid}",
        "flow_revision": "LatestPublished",
        "offset": {
          "x": -90,
          "y": -130
        },
        "parameters": [
          {
            "type": "string",
            "value": "true",
            "key": "system_down"
          }
        ]
      }
    }
  ],
  "initial_state": "Trigger",
  "flags": {
    "allow_concurrent_calls": true
  }
})}